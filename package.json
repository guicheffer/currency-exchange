{
  "name": "currency-exchange",
  "version": "0.1.0",
  "private": true,
  "author": "Jo√£o Guilherme <hi@guicheffer.me>",
  "homepage": "http://guicheffer.me/",
  "repository": {
    "type": "git",
    "url": "https://github.com/guicheffer/currency-exchange/"
  },
  "var": {
    "dist": "./build/",
    "port": "3000",
    "runner": "react-scripts",
    "server": "http-server"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@reduxjs/toolkit": "1.2.5",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.7",
    "http-server": "0.12.3",
    "koa": "2.13.0",
    "koa-router": "9.4.0",
    "node-fetch": "2.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.3",
    "redux-saga": "1.1.3",
    "reselect": "4.0.0",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@types/koa": "2.11.5",
    "@types/koa-router": "7.4.1",
    "@types/koa__cors": "3.0.2",
    "@types/node-fetch": "2.5.7",
    "@types/reselect": "2.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "2.0.5",
    "ts-node": "9.0.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "14.*",
    "yarn": "1.*"
  },
  "scripts": {
    "api": "API_PORT=3001 nodemon --watch 'server/api/*' -e ts --exec ts-node --project server/tsconfig.json server/api/index.ts",
    "start": "$npm_package_var_runner start",
    "build": "$npm_package_var_runner build",
    "test": "$npm_package_var_runner test",
    "serve": "$npm_package_var_server $npm_package_var_dist -p $npm_package_var_port"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
